{
  "name": "medconnectapp",
  "version": "0.0.1",
  "private": true,
  "rnpm": {
    "assets": [
      "./src/assets/Fonts"
    ]
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "react-native run-android",
    "android:apk": "cd android && ./gradlew assembleRelease",
    "android:install": "adb install android/app/build/outputs/apk/app-release.apk",
    "clear": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "devtools": "react-devtools",
    "gen:graphql": "gql-gen --config codegen.yml",
    "ios": "react-native run-ios",
    "ios:beta": "(cd ios/ && fastlane beta)",
    "lint": "tslint --project tsconfig.json",
    "lint:staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "postversion": "react-native-version",
    "bump:stable-release": "NEXT_STABLE=\"$(semver $npm_package_version -i release)\"; npm version $NEXT_STABLE",
    "bump:stable-major": "NEXT_STABLE=\"$(semver $npm_package_version -i major)\"; npm version $NEXT_STABLE",
    "bump:stable-minor": "NEXT_STABLE=\"$(semver $npm_package_version -i minor)\"; npm version $NEXT_STABLE",
    "bump:stable-patch": "NEXT_STABLE=\"$(semver $npm_package_version -i patch)\"; npm version $NEXT_STABLE",
    "bump:beta-release": "NEXT_BETA=\"$(semver $npm_package_version -i prerelease --preid beta)\"; npm version $NEXT_BETA",
    "bump:beta-major": "NEXT_BETA=\"$(semver $npm_package_version -i premajor --preid beta)\"; npm version $NEXT_BETA",
    "bump:beta-minor": "NEXT_BETA=\"$(semver $npm_package_version -i preminor --preid beta)\"; npm version $NEXT_BETA",
    "bump:beta-patch": "NEXT_BETA=\"$(semver $npm_package_version -i prepatch --preid beta)\"; npm version $NEXT_BETA"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.4.0",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-context": "^1.0.17",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-upload-client": "^10.0.1",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "moment": "^2.24.0",
    "react": "16.8.6",
    "react-apollo": "^2.5.6",
    "react-apollo-hooks": "^0.4.5",
    "react-native": "0.59.8",
    "react-native-biometrics": "^1.5.2",
    "react-native-device-info": "^2.0.1",
    "react-native-dynamic-vector-icons": "0.0.3",
    "react-native-easy-toast": "^1.2.0",
    "react-native-elements": "^1.1.0",
    "react-native-gesture-handler": "^1.2.1",
    "react-native-maps": ">=0.24.2",
    "react-native-modal-datetime-picker": "^7.4.2",
    "react-native-pose": "^0.9.1",
    "react-native-ratings": "^6.3.1",
    "react-native-reanimated": "^1.0.1",
    "react-native-signature-capture": "^0.4.9",
    "react-native-snap-carousel": "^3.8.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-swiper": "^1.5.14",
    "react-native-vector-icons": "^6.4.2",
    "react-navigation": "^3.11.0",
    "react-navigation-hooks": "^1.0.0-alpha.2",
    "rn-sliding-up-panel": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@graphql-codegen/add": "^1.2.0",
    "@graphql-codegen/cli": "^1.2.0",
    "@graphql-codegen/fragment-matcher": "^1.2.0",
    "@graphql-codegen/typescript": "^1.2.0",
    "@graphql-codegen/typescript-operations": "^1.2.0",
    "@graphql-codegen/typescript-react-apollo": "^1.2.0",
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.18",
    "@types/react-native": "^0.57.60",
    "@types/react-native-vector-icons": "^6.4.0",
    "@types/react-navigation": "^3.0.7",
    "@types/react-test-renderer": "^16.8.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "danger": "^7.1.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-okonet": "^7.0.2",
    "eslint-config-shellscape": "^2.0.2",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-native": "^3.7.0",
    "eslint-plugin-relay": "^1.3.1",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-runner-eslint": "^0.7.3",
    "lint-staged": "^8.1.7",
    "metro-react-native-babel-preset": "^0.54.1",
    "nock": "^10.0.6",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.1",
    "react-devtools": "^3.6.1",
    "react-native-testing-library": "^1.7.0",
    "react-native-version": "^3.1.0",
    "react-test-renderer": "16.8.6",
    "semver": "^6.1.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5",
    "typescript-eslint-parser": "^22.0.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native|native-base-shoutem-theme|react-navigation-stack|@react-navigation|react-navigation-fluid-transitions)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFiles": [
      "./jestSetup.js"
    ]
  }
}
